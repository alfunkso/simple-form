{
  "extends": [
    "tslint-config-prettier",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-airbnb"
  ],
  "rules": {
    "function-name": false,
    "jsx-no-multiline-js": false
  },
  "defaultSeverity": "warning",
  "jsRules": true,
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}